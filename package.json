{
  "name": "bloqit-api",
  "version": "1.0.0",
  "description": "An API for Bloqit",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "test": "jest --forceExit --detectOpenHandles",
    "seed:test": "ts-node src/scripts/seed.ts",
    "test:integration": "npm run seed:test && npm run test"
  },
  "keywords": [
    "express",
    "typescript",
    "mongoose",
    "jest",
    "supertest",
    "api"
  ],
  "author": "Ricardo Ferreira",
  "license": "MIT",
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "uuid": "^11.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.4.2",
    "@types/supertest": "^2.0.12",
    "jest": "^29.6.1",
    "mongodb-memory-server": "^8.12.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
